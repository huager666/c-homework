#include <iostream>
#include<string.h>
using namespace std;

int main()
{

	
	
		int t = 0;
		cin >> t;
		for (int i = 0; i < t; i++)
		{
				int len = 0;
				cin >> len;
				int mid = 0;
				int* score = new int[len];
				for (int i = 0; i < len; i++)
				{
					cin >> *(score + i);
				}
				if ((len % 2) == 0)
				{
					mid = len / 2 + 1;
				}
				else
				{
					mid = (len+1) / 2 ;
				}
				cout << *(score + mid-2)<<" " <<* (score + mid) << endl;
				int find = 0;
				cin >> find;
				cout << *(score + find-1) << endl;
		}

}

