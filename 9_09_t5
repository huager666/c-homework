#include <iostream>
#include<string.h>
using namespace std;

int main()
{



	int t = 0;
	cin >> t;
	for (int i = 0; i < t; i++)
	{	
		
		char* p=new char[50];
		char* q = new char[50];
		char* s = new char[50];
		int j= 0;
		cin >> p;
		cin >> q;

	
		for (int i = 0; i < strlen(p); i++)
		{
			
			
				
				
				if (*(p + i) >= 'A' && *(p + i) <= 'Z')
				{
					*(s + i) = *(p + i) + *(q + j) - '0';
					if (*(s + i) > 'Z')
					{
						*(s + i) = *(s + i) - 26;
					}
					j++;
				}
				if (*(p + i) >= 'a' && *(p + i) <= 'z')
				{
					*(s + i) = *(p + i) + *(q + j) - '0';	
					if (*(s + i) > 'z')
					{
						*(s + i) = *(s + i) - 26;
					}

					j++;
				}
				if (j == strlen(q))
				{
					j = 0;
				}



		}
		*(s + strlen(p) + 1) = '\0';
		cout << s << endl;
	
		
		
		

		
	}
}
